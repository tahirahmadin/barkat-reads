{
  "expo": {
    "name": "Barkat Learn",
    "slug": "barkat-learn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f2ede8"
    },
    "ios": {
      "bundleIdentifier": "com.barkat.reads",
      "supportsTablet": true,
      "buildNumber": "1"
    },
    "android": {
      "package": "com.barkat.reads",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#f2ede8"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}